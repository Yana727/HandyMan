@{
    ViewData["Title"] = "Price List";
}

@model List<PriceModel>

<h2>@ViewData["Title"]</h2>
<ul>
        @foreach (var item in Model)
        {
            <li> @item.Service</li> 
            <li>@item.Info</li> 
        }
</ul>

<p>Use this area to provide additional information.</p>
